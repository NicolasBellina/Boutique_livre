    "@vitest/ui": "^1.6.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "happy-dom": "^12.10.3",
    "prettier": "^3.1.1",
    "vite": "^5.0.10",
    "vitest": "^1.6.1"
